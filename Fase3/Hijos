create 

int bot a,b,c,d,f,g 
   on ~(true/\false)\/true\/~(true):
       read as x.
       read as p.
    end
   
    on deactivation:
        read.
    end

    on activation:
        collect as no.
    end

    on default:
        collect.
    end

end

bool bot m,z  
     on activation:
         store me.
     end
end 

execute
	activate d.
  $- create
      int bot bot1,bot2 
      on activation:
          store x+1.
          read as y.
      end
   end
   execute
       deactivate bot1.
   end
   advance a.
   if (a<3)\/true/\(2/=7):
        activate b.
   end -$   
end
